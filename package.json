{
  "type": "module",
  "name": "bird-app",
  "version": "1.0.0",
  "description": "A Twitter clone",
  "main": "index.js",
  "scripts": {
    "start": "nodemon server",
    "server": "nodemon server",
    "client": "npm start --prefix client",
    "data:import": "node server/seeder",
    "data:destroy": "node server/seeder -d",
    "dev": "concurrently \"npm run server\" \"npm run client\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tarunsinghdev/bird-app.git"
  },
  "keywords": [
    "bird-app",
    "twitter",
    "clone"
  ],
  "author": "Tarun Singh",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tarunsinghdev/bird-app/issues"
  },
  "homepage": "https://github.com/tarunsinghdev/bird-app#readme",
  "dependencies": {
    "bcrypt": "^5.0.1",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.13.2",
    "validator": "^13.6.0"
  },
  "devDependencies": {
    "colors": "^1.4.0",
    "concurrently": "^6.2.0",
    "dotenv": "^10.0.0",
    "morgan": "^1.10.0",
    "nodemon": "^2.0.12"
  }
}
